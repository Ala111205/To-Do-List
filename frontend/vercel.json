{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" },
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/reset-password/.*", "dest": "/reset.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}